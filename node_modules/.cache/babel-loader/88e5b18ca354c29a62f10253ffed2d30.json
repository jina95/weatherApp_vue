;\nimport Vuex from 'vuex';\nimport { fetchWeatherList } from '../api/index';\n\nVue.use(Vuex);\n\nexport default new Vuex.Store({\n  state: {\n    weatherList: {},\n  },\n  mutations: {\n    SET_WEATHER(state, data) {\n      state.weatherList = data;\n    },\n  },\n  actions: {\n    FETCH_WEATHER(context, cityname) {\n      return fetchWeatherList(cityname)\n        .then(res => {\n          context.commit('SET_WEATHER', res.data);\n          return res;\n        })\n        .catch(error => {\n          console.log(error);\n        });\n    },\n  },\n  modules: {},\n});\n"]}]}